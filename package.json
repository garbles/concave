{
  "name": "concave",
  "version": "0.0.16",
  "description": "A Lens-like interface for state management in React",
  "main": "dist/create-lens.js",
  "module": "dist/create-lens.mjs",
  "types": "dist/create-lens.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/create-lens.d.ts"
      ],
      "react": [
        "./dist/react.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/create-lens.mjs",
      "require": "./dist/create-lens.js"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Gabe Scholz",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "react",
    "lens",
    "optics",
    "state",
    "state-management",
    "store"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/garbles/concave.git"
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "build": "tsup src/create-lens.ts src/react.ts",
    "prepublish": "yarn test && yarn typecheck && yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/react": "^13.0.0-alpha.5",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "jest": "^27.4.5",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "ts-jest": "^27.1.2",
    "tsup": "^5.11.8",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@types/react": "^17.0.37",
    "react": "^18.0.0-rc.0"
  }
}
